# Environment Variables Template
# Copy this file to .env and fill in the actual values

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/seezee

# AI Services (REQUIRED for AI features)
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-proj-your-key-here

# Google Maps (Optional - for geocoding leads)
GOOGLE_MAPS_API_KEY=your-google-maps-key

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Email Configuration (Resend)
RESEND_API_KEY=re_your-resend-api-key
RESEND_FROM_EMAIL=sean@see-zee.com

# File Upload Configuration (UploadThing)
UPLOADTHING_TOKEN=your-uploadthing-token

# Marketing & Outreach API Keys
GOOGLE_PLACES_API_KEY=your-google-places-api-key
APOLLO_API_KEY=your-apollo-api-key
HUNTER_API_KEY=your-hunter-api-key

# GitHub Integration (Optional - for team commit feed events and Command Center)
# Create a personal access token at: https://github.com/settings/tokens
# Required scopes: repo, read:user, read:org (if tracking org activity)
GITHUB_TOKEN=ghp_your-github-personal-access-token
GITHUB_USERNAME=SeanSpon
GITHUB_REPO_SLUG=owner/repository-name

# Vercel Integration (Optional - for Command Center deployment tracking)
# Get token from: https://vercel.com/account/tokens
# Your workspace: seanspons-projects (personal account)
VERCEL_TOKEN=your-vercel-api-token
# VERCEL_TEAM_ID is optional - only needed if using a team account
# Leave commented out to use your personal account
# VERCEL_TEAM_ID=team_xxxxx  # Only if using team account
VERCEL_WEBHOOK_SECRET=your-random-webhook-secret-min-32-chars  # For webhook signature verification

# Google Workspace Integration (Optional - for Command Center email management)
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
GOOGLE_ADMIN_EMAIL=sean@yourdomain.com
GOOGLE_WORKSPACE_DOMAIN=yourdomain.com

# Feature Flags
SEEZEE_FEATURE_FEED=true
SEEZEE_FEATURE_MESSAGES=true
SEEZEE_FEATURE_INVOICES=true
SEEZEE_FEATURE_BILLING=true